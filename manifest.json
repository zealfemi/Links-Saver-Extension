{
    "manifest_version": 3,
    "version": "0.1.0",
    "name": "Links (URLs) Saver",
    "description": "Save URL for reference.",
    "icons": {
        "16": "/images/16x16.png",
        "24": "/images/24x24.png",
        "32": "/images/32x32.png",
        "64": "/images/64x64.png",
        "128": "/images/128x128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "/images/16x16.png",
            "24": "/images/24x24.png",
            "32": "/images/32x32.png",
            "64": "/images/64x64.png",
            "128": "/images/128x128.png"
        }
    },
    "permissions": [
        "tabs"
    ]
}
